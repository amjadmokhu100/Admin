


@model IEnumerable<test3.Models.ServiceModel>

    @{
        ViewBag.Title = " Service";
    }



    <div class="container-fluid">
        <div class="row">
            <div class="col-12">
                <div class="page-title-box">
                    <div class="page-title-right">
                        <ol class="breadcrumb m-0">
                            <li class="breadcrumb-item">Main</li>
                            <li class="breadcrumb-item">Services</li>

                        </ol>
                    </div>
                    <h4 class="page-title"> Services</h4>
                </div>
            </div>
        </div>
        <div class="row">
            <div class="col-12">
                <div class="card">
                    <div class="card-body">
                        <p class="btn btn-danger"">
                            @Html.ActionLink("Create New", "Create")
                        </p>
                        <table class="table">
                            <tr>
                                <th>
                                    @Html.DisplayNameFor(model => model.Photo)
                                </th>
                                <th>
                                    @Html.DisplayNameFor(model => model.Name)
                                </th>
                                <th>
                                    @Html.DisplayNameFor(model => model.Description)
                                </th>
                                <th>
                                    @Html.DisplayNameFor(model => model.NormalPrice)
                                </th>
                                <th>
                                    @Html.DisplayNameFor(model => model.NormalHour)
                                </th>
                                <th>
                                    @Html.DisplayNameFor(model => model.FastPrice)
                                </th>
                                <th>
                                    @Html.DisplayNameFor(model => model.FastHour)
                                </th>
                                <th>
                                    @Html.DisplayNameFor(model => model.Photo)
                                </th>

                                <th></th>
                            </tr>

                            @foreach (var item in Model)
                            {
                                <tr>
                                    <td>
                                        <img src="~/Upload/Services/@item.Photo" class="img-thumbnail">
                                    </td>
                                    <td>
                                        @Html.DisplayFor(modelItem => item.Name)
                                    </td>
                                    <td>
                                        @Html.DisplayFor(modelItem => item.Description)
                                    </td>
                                    <td>
                                        @Html.DisplayFor(modelItem => item.NormalPrice)
                                    </td>
                                    <td>
                                        @Html.DisplayFor(modelItem => item.NormalHour)
                                    </td>
                                    <td>
                                        @Html.DisplayFor(modelItem => item.FastPrice)
                                    </td>
                                    <td>
                                        @Html.DisplayFor(modelItem => item.FastHour)
                                    </td>



                                    <td>
                                        @Html.ActionLink("Edit", "Edit", new { id = item.Id }) |
                                        @Html.ActionLink("Details", "Details", new { id = item.Id }) |
                                        @Html.ActionLink("Delete", "Delete", new { id = item.Id })
                                    </td>
                                </tr>
                            }

                        </table>

                </div>

            </div>
        </div>
    </div>

</div>

